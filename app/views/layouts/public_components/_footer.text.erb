
---

<%= t('mail.need_to_reach') %>

<% if @contacts[:whatsapp_url].present? %>
  Whatsapp: <%= @contacts.dig!(:whatsapp_url) %>
<% end %>

<% if @contacts[:address].present? %>
  Location: <%= @contacts.dig!(:address) %>
<% end %>

<% if @contacts[:homepage_url].present? %>
  Our website: <%= @contacts.dig!(:homepage_url) %>
<% end %>

<% if @contacts[:email].present? %>
  Email: <%= @contacts.dig!(:email) %>
<% end %>

<% if @contacts[:whatsapp_number].present? %>
  Call us on <%= @contacts.dig!(:whatsapp_number) %>
<% end %>

<% if @contacts[:facebook_url].present? %>
  Facebook: <%= @contacts.dig!(:facebook_url) %>
<% end %>

<% if @contacts[:instagram_url].present? %>
  Instagram: <%= @contacts.dig!(:instagram_url) %>
<% end %>

<% if @contacts[:tripadvisor_url].present? %>
  Tripadvisor: <%= @contacts.dig!(:tripadvisor_url) %>
<% end %>